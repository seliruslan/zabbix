apiVersion: 1

datasources:
  - name: Zabbix
    type: alexanderzobnin-zabbix-datasource
    access: proxy
    isDefault: true
    editable: true
    url: {{ zabbix_api_url | quote }}
    jsonData:
      username: {{ zabbix_default_user | quote }}
    secureJsonData:
      password: {{ zabbix_default_password | quote }}
